Program doisDividem ;

Var
	num1: real;
	num2: real;

 Begin
 
 	writeln('Programa para o cálculo do quociente entre dois números:');
	writeln('Digite o primeiro número:');
	readln(num1);
	
	writeln('Digite o segundo número:');
	readln(num2);
	
	writeln('O resultado da divisão entre o primeiro e o segundo número é:');
	write(num1 / num2);
  
 End.